<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:at="http://mycompany.com/airport"
           xmlns:error="http://mycompany.com/common/error"
           targetNamespace="http://mycompany.com/airport/airportService"
           elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">

    <xs:import namespace="http://mycompany.com/airport" schemaLocation="../../shared-xsds/airport/Airport-1.xsd"/>
    <xs:import namespace="http://mycompany.com/common/error" schemaLocation="../../shared-xsds/common/error/Error-1.xsd"/>

    <xs:element name="GetAirportsRequest">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>Get all suitable Airports.</xs:documentation>
            </xs:annotation>
        </xs:complexType>
    </xs:element>
    <xs:element name="GetAirportsResponse">
        <xs:annotation>
            <xs:documentation>Returns a list of valid airports</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:choice>
                <xs:element ref="at:Airports" maxOccurs="1" minOccurs="0"/>
                <xs:element ref="error:ErrorList"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>
</xs:schema>
